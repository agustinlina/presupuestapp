{
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.png", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
    
  ],
  "routes": [
    { "src": "/api", "dest": "/generar-pdf.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}

